{
  "name": "littlegiqntbot",
  "author": "littlegiqnt",
  "scripts": {
    "start": "cross-env NODE_ENV=production NODE_PATH=./dist node dist/index.js",
    "build": "swc ./src -d dist",
    "dev": "rm -rf ./dist && swc ./src -d dist && concurrently -n W: \"npm:watch-*\"",
    "watch-swc": "swc ./src -d dist --watch",
    "watch-node": "cross-env NODE_ENV=development NODE_PATH=./dist nodemon ./dist/index.js"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.51",
    "@types/node": "^18.15.11",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "bufferutil": "^4.0.7",
    "discord.js": "^14.9.0",
    "mongoose": "^6.10.5",
    "node-schedule": "^2.1.1",
    "utf-8-validate": "^5.0.10"
  }
}
