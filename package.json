    "@types/node-schedule": "^2.1.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-plugin-prefer-arrow-functions": "^3.2.4",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node-dev": "^2.0.0",
